{
  "name": "pokeviewer",
  "description": "Simple Pokemon viewer",
  "author": "Andy Barron <AndrewLBarron@gmail.com>",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "node server.js",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
    "start": "npm-run-all build serve"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "chroma-js": "^1.2.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "express": "^4.14.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vue": "^2.0.1",
    "vue-router": "^2.1.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "engines": {
    "node": "7.x.x"
  }
}
